<div>
    <%= form_tag nil, method: 'get', class: 'form-group form-inline' do %>
      <%= text_field_tag :search, params[:search], class: "nav-link", placeholder: "Search" %>
      <%= button_tag(:id =>'search-button', :class => "btn btn-none", placeholder: "Search" ) do %>
        <i class="fa fa-search"></i>
      <% end %>
    <% end %>
</div>

<% @workouts.each do |workout| %>
  <% @user_workouts.each do |user_workout| %>
    <% if user_workout.workout != workout %>
      <div>
        <p><%= user_friendly_range(workout.start_time) %> to <%= readable_time(workout.end_time) %> - <%=link_to( workout.user.first_name, workout_path(workout.id)) %></p>
      </div>
    <% end %>
  <% end %>
<% end%>